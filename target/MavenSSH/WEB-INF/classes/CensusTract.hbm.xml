<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.entity.CensusTract" table="census_tract" schema="chronic">
        <composite-id mapped="true" class="com.entity.CensusTractPK">
            <key-property name="ctId" column="CT_id"/>
            <key-property name="ctYear" column="CT_year"/>
        </composite-id>
        <property name="ndId" column="Nd_id"/>
        <property name="ctUid" column="CT_UID"/>
    </class>
</hibernate-mapping>