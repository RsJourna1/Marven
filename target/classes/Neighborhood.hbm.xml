<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.entity.Neighborhood" table="neighborhood" schema="chronic">
        <id name="ndId" column="Nd_id"/>
        <property name="ndName" column="Nd_name"/>
        <property name="ndArea" column="Nd_area"/>
        <property name="ndCenterX" column="Nd_centerX"/>
        <property name="ndCenterY" column="Nd_centerY"/>
    </class>
</hibernate-mapping>